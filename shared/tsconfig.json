{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "sourceMap": true,
    "declaration": true,
    "target": "es6",
    "strict": true,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true
  }
}
